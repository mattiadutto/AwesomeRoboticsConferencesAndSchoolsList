{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"shortName": {
				"type": "string"
			},
			"name": {
				"type": "string"
			},
			"start": {
				"type": "string",
				"format": "date"
			},
			"end": {
				"type": "string",
				"format": "date"
			},
			"deadline": {
				"oneOf": [{
						"type": "string",
						"maxLength": 0
					},
					{
						"type": "string",
						"format": "date"
					}
				]
			},
			"city": {
				"type": "string"
			},
			"country": {
				"type": "string"
			},
			"link": {
				"oneOf": [{
						"type": "string",
						"maxLength": 0
					},
					{
						"type": "string",
						"format": "uri"
					}
				]
			},
			"addressLink": {
				"oneOf": [{
						"type": "string",
						"maxLength": 0
					},
					{
						"type": "string",
						"format": "uri"
					}
				]
			},
			"type": {
				"oneOf": [{
						"type": "string",
						"maxLength": 0
					},
					{
						"enum": ["Conference", "Workshop", "School"]
					}
				]
			},
			"note": {
				"type": "string"
			}
		},
		"additionalProperties": false,
		"required": ["shortName", "name", "start", "end", "city", "country", "type"]
	}
}
